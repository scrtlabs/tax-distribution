{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "beneficiaries",
    "decimal_places_in_weights"
  ],
  "properties": {
    "beneficiaries": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Beneficiary"
      }
    },
    "decimal_places_in_weights": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "tax_denom": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "definitions": {
    "Beneficiary": {
      "type": "object",
      "required": [
        "address",
        "weight"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "weight": {
          "type": "integer",
          "format": "uint16",
          "minimum": 0.0
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
